<%- include("partials/header") -%>
<h1>Hello</h1>
<h1>Register</h1>

<form action="/contact" method="POST">
  <div class="form-group">
    <label for="email">Email</label>
    <input type="email" class="form-control" name="username">
  </div>
  <div class="form-group">
    <label for="password">Password</label>
    <input type="password" class="form-control" name="password">
  </div>
  <button type="submit" class="btn btn-dark">Register</button>
</form>


<iframe width="560" height="315" src="https://www.youtube.com/embed/ljDRzQz3ULE" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
